{
  "AppSettings": {
    "UseSsl": false
  },
  "CkeditorOptions": {
  "CustomConfigPath": "/cr/js/cloudscribe-ckeditor-config.min.js",
  "FileBrowseUrl": "/filemanager/filedialog?type=file",
  "ImageBrowseUrl": "/filemanager/filedialog?type=image",
  "DropFileUrl": "/filemanager/dropfile"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "EO4Coding": "Trace",
      //"System": "Information",
      //"Microsoft": "Information"
      "Saaskit": "Trace"
    }
  },

  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": ".\\logs\\log-{Date}.txt",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] [{SourceContext}] [{EventId}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },

  "PageEditOptions": {
    "AlwaysShowDeveloperLink": true,
    "DeveloperAllowedRole": "Admins"
  }
}
